{
    "manifest_version": 3,
    "name": "Amazon ACOS",
    "version": "1.0",
    "description": "Display target bids based on set ACOS",
    "action":
    {},
    "icons":
    {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "web_accessible_resources": [
    {
        "resources": ["code.jquery.com_jquery-3.7.0"],
        "matches": ["*://*/*"]
    }],
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "options_page": "options.html",
    "background":
    {
        "service_worker": "background.js"
    }
}